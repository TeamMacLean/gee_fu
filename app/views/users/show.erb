<% @users.each do |user| %>
<div class=<% if user.admin %>"boxedin userAdmin"<% else %>"boxedin userNormal"<% end %>>
	<p><%= user.first_name %></p>
	<p><%= user.last_name %></p>
	<p><%= user.email %></p>
	
	<% if !user.admin %>
	<a href=<%=edit_user_registration_path%> class="btn btn-into" >Edit</a>
    <p><%= button_to "Cancel account", registration_path(user), :data => { :confirm => "Are you sure?" }, :method => :delete, :class => "btn btn-danger" %></p>
	<% end %>
</div>
<hr>
<% end %>