<% @users.each do |user| %>
<div class=<% if user.admin %>userAdmin<% else %>userNormal<% end %>>
	<p><%= user.first_name %></p>
	<p><%= user.last_name %></p>
	<p><%= user.email %></p>
	
	<% if !user.admin %>
	<a href="#" class="btn btn-danger" data-confirm="Are you sure?" data-method="delete" rel="nofollow">Delete</a>
	<% end %>
</div>
<hr>
<% end %>